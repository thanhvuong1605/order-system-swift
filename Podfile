# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Order System Mobile' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Order System Mobile
  #podfile for import 3rd party library to the project
  pod 'Stripe', '~> 8.0.7'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON'
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git'




  target 'Order System MobileTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Order System MobileUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
